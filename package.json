{
  "name": "backlogger",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "prisma-dev-run -- dotenvx run --convention=nextjs -- next dev --turbopack",
    "build": "dotenvx run --convention=nextjs -- next build --turbopack",
    "start": "dotenvx run --convention=nextjs -- next start",
    "format": "prettier --write . --cache; \n prisma format",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "encrypt": "dotenvx encrypt --env-file .env --exclude-key DATABASE_URL",
    "decrypt": "dotenvx decrypt --env-file .env"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "better-auth": "^1.3.32",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@eslint/eslintrc": "^3",
    "@prisma/adapter-pg": "^6.15.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-sql": "^0.19.2",
    "prisma": "^6.18.0",
    "prisma-dev-run": "file:./prisma-dev-run",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "dependenciesComments": {
    "@eslint/eslintrc": "This could probably be removed at some point if the config file is updated"
  },
  "engineStrict": true,
  "engines": {
    "node": "24.x",
    "npm": "11.x"
  }
}