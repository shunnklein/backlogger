{
  "name": "backlogger",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx @dotenvx/dotenvx run -- next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "format": "prettier --write . --cache",
    "lint": "npx --node-options='--experimental-strip-types' eslint --flag unstable_native_nodejs_ts_config",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "6.15.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@eslint/eslintrc": "^3",
    "@prisma/adapter-pg": "^6.15.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "prettier": "^3.6.2",
    "prisma": "6.15.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "dependenciesComments": {
    "tsx": "only for the prisma migration seed. if that gets removed or we move to a node version that supports typescript this can be removed"
  },
  "engineStrict": true,
  "engines": {
    "node": "24.x",
    "npm": "11.x"
  }
}
